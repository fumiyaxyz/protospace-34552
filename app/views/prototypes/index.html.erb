<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <div>こんにちは、</div>
        <div><%= link_to "#{current_user.name}", user_path(current_user), class: :greeting__link%></div>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %> 
    </div>
  </div>
</main>
